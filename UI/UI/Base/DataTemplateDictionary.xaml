<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewModel="clr-namespace:Sando.UI.ViewModel"
                    xmlns:view="clr-namespace:Sando.UI.View">
    <DataTemplate DataType="{x:Type viewModel:MenuViewModel}">
        <view:MenuView></view:MenuView>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModel:SearchViewModel}">
        <view:SearchView></view:SearchView>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModel:SearchResultViewModel}">
        <view:SearchResultView></view:SearchResultView>
    </DataTemplate>
</ResourceDictionary>